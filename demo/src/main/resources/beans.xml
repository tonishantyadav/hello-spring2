<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
  <bean id="alien" class="com.example.dummy.Alien" autowire="byType">
    <property name="age" value="21"/>
    <property name="salary" value="1000"/>
  </bean>
  <bean id="computer1" class="com.example.dummy.Laptop" primary="true" />
  <bean id="computer2" class="com.example.dummy.Desktop" lazy-init="true"/>
</beans>